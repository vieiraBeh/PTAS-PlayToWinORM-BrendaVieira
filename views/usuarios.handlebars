<h1>Usuários Cadastrados:</h1>

<a href="/usuarios/novo">Cadastrar novo usuário.</a>
<table border="1">
   <tr>
    <td>Id:</td>
    <td>Nickname:</td>
    <td>Nome:</td>
   </tr>
   {{#each usuarios}}
   <tr>
    <td>{{this.id}}</td>
    <td>{{this.nickname}}</td>
    <td>{{this.nome}}</td>
    <td>
      <a href="/usuarios/{{this.id}}/update">Atualizar usuário.</a>
      <form action="/usuarios/{{this.id}}/delete" method="post">
      <input type="submit" value="EXCLUIR"/>
      </form>
    </td>
   </tr>
   {{/each}}
</table>
